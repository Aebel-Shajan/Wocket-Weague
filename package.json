{
	"name": "wocket-weague",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "tsc --noEmit -p tsconfig.json && biome lint --diagnostic-level=error --no-errors-on-unmatched src/",
		"lint:fix": "biome check --write --unsafe src/",
		"lint:staged": "lint-staged",
		"prepare": "husky"
	},
	"lint-staged": {
		"*": "biome check --write --error-on-warnings --no-errors-on-unmatched src/"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.2",
		"husky": "9.1.6",
		"lint-staged": "^15.2.10",
		"typescript": "^5.5.3",
		"vite": "^5.4.1"
	},
	"dependencies": {
		"@dimforge/rapier3d-compat": "^0.14.0",
		"@types/three": "^0.168.0",
		"three": "^0.168.0"
	}
}
